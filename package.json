{
  "name": "@jackkav/react-countdown",
  "version": "1.1.5",
  "description": "A reusable React component to abstract away the countdown logic so you can focus on the layout.",
  "repository": {
    "type": "git",
    "url": "https://github.com/jackkav/react-countdown.git"
  },
  "main": "dist/index.js",
  "author": "Jack Kavanagh",
  "license": "MIT",
  "keywords": [
    "countdown",
    "react",
    "babel7",
    "render-props",
    "react-component"
  ],
  "homepage": "https://github.com/jackkav/react-countdown",
  "scripts": {
    "test": "jest",
    "build": "babel src  --out-dir dist --ignore 'src/**/*.test.js'",
    "build:watch": "babel src --out-dir dist --ignore 'src/**/*.test.js' --watch"
  },
  "husky": {
    "hooks": {
      "pre-commit": "jest && babel src  --out-dir dist --ignore 'src/**/*.test.js'"
    }
  },
  "devDependencies": {
    "@babel/cli": "7.1.5",
    "@babel/core": "7.1.6",
    "@babel/preset-env": "7.1.6",
    "@babel/preset-react": "7.0.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "29.7.0",
    "husky": "1.1.4",
    "jest": "29.7.0",
    "react": "16.6.3",
    "regenerator-runtime": "0.13.1"
  },
  "dependencies": {
    "@babel/polyfill": "^7.0.0"
  }
}
